{
  "name": "hel",
  "version": "4.11.3",
  "description": "",
  "keywords": [],
  "author": "fantasticsoul",
  "main": "index.js",
  "scripts": {
    "build": "npm run build_hel && npm run build_proxy && npm run build_core",
    "build_core": "pnpm --filter=hel-micro-core run build",
    "build_hel": "pnpm --filter=hel-micro run build",
    "build_proxy": "pnpm --filter=hel-lib-proxy run build",
    "commitlint": "commitlint -e $HUSKY_GIT_PARAMS",
    "cpdoc": "rm -rf ../my-opensource/hel-gh-pages/* && cp -r ./doc/build/* ../my-opensource/hel-gh-pages",
    "doc": "pnpm --filter=hel-doc run build && npm run cpdoc",
    "format": "prettier --cache --write .",
    "prepare": "husky install",
    "demo:host": "pnpm --filter=demos/main run dev",
    "demo:remote": "pnpm --filter=demos/remote run dev",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check"
  },
  "dependencies": {
    "hel-html-parser": "workspace:*",
    "hel-lib-proxy": "workspace:*",
    "hel-micro": "workspace:*",
    "hel-micro-core": "workspace:*",
    "hel-micro-react": "workspace:*",
    "hel-types": "workspace:*",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-is": "^18.2.0"
  },
  "devDependencies": {
    "@jest/transform": "29",
    "@types/node": "^14.14.20",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "@babel/core": "^7.19.3",
    "@types/jest": "27.5.2",
    "cross-env": "^7.0.3",
    "esbuild-plugin-module-css": "^0.1.1",
    "fs-extra": "^10.1.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-environment-jsdom": "27.5.1",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "resolve": "^1.22.1",
    "rollup": "^2.23.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "28.0.8",
    "tslib": "^1.14.1",
    "tsup": "^8.5.0",
    "typescript": "4.8.3"
  },
  "volta": {
    "node": "20.19.6",
    "pnpm": "8.3.1"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "^16.8.0",
      "@types/react-dom": "^16.8.0"
    }
  }
}
