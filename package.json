{
  "name": "hel",
  "version": "4.11.3",
  "description": "",
  "keywords": [],
  "author": "fantasticsoul",
  "main": "index.js",
  "scripts": {
    "build": "npm run build_hel && npm run build_proxy && npm run build_core",
    "build_core": "pnpm --filter=hel-micro-core run build",
    "build_hel": "pnpm --filter=hel-micro run build",
    "build_proxy": "pnpm --filter=hel-lib-proxy run build",
    "commitlint": "commitlint -e $HUSKY_GIT_PARAMS",
    "cpdoc": "rm -rf ../my-opensource/hel-gh-pages/* && cp -r ./doc/build/* ../my-opensource/hel-gh-pages",
    "doc": "pnpm --filter=hel-doc run build && npm run cpdoc",
    "prepare": "husky install",
    "demo:host": "pnpm --filter=demos/main run dev",
    "demo:remote": "pnpm --filter=demos/remote run dev",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check"
  },
  "dependencies": {
    "hel-html-parser": "workspace:*",
    "hel-lib-proxy": "workspace:*",
    "hel-micro": "workspace:*",
    "hel-micro-core": "workspace:*",
    "hel-micro-react": "workspace:*",
    "hel-types": "workspace:*",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/plugin-transform-class-properties": "^7.28.6",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^27.5.0",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "cross-env": "^10.1.0",
    "esbuild-plugin-module-css": "^0.1.1",
    "fs-extra": "^11.3.3",
    "husky": "9.1.7",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "rollup": "^4.57.0",
    "ts-jest": "^28.0.1",
    "tsdown": "^0.20.1",
    "tslib": "^2.8.1",
    "typescript": "5.9.3"
  },
  "volta": {
    "node": "20.19.6",
    "pnpm": "8.3.1"
  }
}
