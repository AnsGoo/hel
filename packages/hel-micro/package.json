{
  "name": "hel-micro",
  "version": "4.15.2",
  "description": "A module federation SDK which is unrelated to tool chain for module consumer.",
  "keywords": [
    "hel",
    "hel-micro",
    "module federation",
    "module federation sdk"
  ],
  "bugs": "https://github.com/Tencent/hel/issues",
  "license": "MIT",
  "author": {
    "name": "fantasticsoul"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tencent/hel.git",
    "directory": "packages/hel-micro"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "tsconfig.json"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "src/index.ts",
  "unpkg": "dist/hel-micro.min.js",
  "scripts": {
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "rollup -c --environment BUILD_ENV:commonjs",
    "build:es": "rollup -c --environment BUILD_ENV:es",
    "build:umd": "rollup -c --environment BUILD_ENV:development",
    "build:umd:min": "rollup -c --environment BUILD_ENV:production",
    "test": "pnpm --filter=hel-micro-core run build:commonjs && jest"
  },
  "dependencies": {
    "hel-html-parser": "^1.1.2",
    "hel-lib-proxy": "^4.12.1",
    "hel-micro-core": "^4.13.1",
    "hel-types": "^4.3.10"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/qs": "^6.9.7",
    "babel-cli": "^6.26.0",
    "fake-indexeddb": "4.0.1",
    "qs": "^6.11.0",
    "rollup": "^3.0.0",
    "typescript": "^4.0.0"
  },
  "peerDependencies": {
    "hel-html-parser": "^1.1.2",
    "hel-lib-proxy": "^4.12.1",
    "hel-micro-core": "^4.13.1",
    "hel-types": "^4.3.10"
  },
  "bundleDependencies": [],
  "deprecated": false
}
