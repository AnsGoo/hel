{
  "name": "mono-comps",
  "version": "0.0.1",
  "description": "hel-mono demo react comps",
  "license": "MIT",
  "author": {
    "name": "fantasticsoul"
  },
  "files": [
    "dist",
    "hel_dist",
    "!src",
    "README.md"
  ],
  "main": "./src/index",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./index.css": "./dist/index.css",
    "./hel": {
      "types": "./src/.hel/entrance/libTypes.ts",
      "import": "./src/.hel/entrance/libTypes.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "dependencies": {
      "@hel-demo/mono-libs": "*"
    },
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      },
      "./index.css": "./dist/index.css",
      "./hel": {
        "types": "./dist/.hel/entrance/libTypes.d.ts",
        "import": "./dist/.hel/entrance/libTypes.mjs",
        "require": "./dist/.hel/entrance/libTypes.js"
      }
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "unpkg": "./dist/index.js"
  },
  "scripts": {
    "start": "node ../../dev/root-scripts/executeStart",
    "start:raw": "node ../../dev/scripts/hel/start.js",
    "start:hel": "node ../../dev/scripts/hel/start.js",
    "start:all": "node ../../dev/scripts/hel/startAll.js",
    "start:for": "node ../../dev/scripts/hel/startFor",
    "build": "pnpm run build:npm",
    "build:hel": "node ../../dev/scripts/hel/build.js",
    "build:all": "node ../../dev/scripts/hel/buildAll.js",
    "build:npm": "tsup",
    "build:nb": "pnpm run build:npm && pnpm run build:hel",
    "build:meta": "node ../../dev/scripts/hel/meta",
    "build:nbm": "cross-env-shell \"pnpm run build:nb && pnpm run build:meta\"",
    "test": "node ../../dev/scripts/test --config ../../dev/config/jest/jest.config.js",
    "test:once": "cross-env ONCE=1 node ../../dev/scripts/test --config ../../dev/config/jest/jest.config.js"
  },
  "dependencies": {},
  "devDependencies": {
    "esbuild-plugin-module-css": "^0.1.1",
    "hash-css-selector": "^1.1.0",
    "hel-lib-proxy": "workspace:*",
    "hel-mono-runtime-helper": "workspace:*",
    "postcss": "^8.4.4",
    "postcss-modules": "^6.0.1"
  },
  "peerDependencies": {
    "hel-lib-proxy": "workspace:*",
    "hel-mono-runtime-helper": "workspace:*"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "volta": {
    "node": "20.19.5",
    "pnpm": "8.6.8"
  },
  "hel": {}
}
