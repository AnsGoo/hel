{
  "name": "hel-micro-mini",
  "version": "0.0.1",
  "description": "A module federation SDK which is unrelated to tool chain for module consumer.",
  "keywords": [
    "hel",
    "hel-micro",
    "hel-micro-mini",
    "module federation",
    "module federation sdk"
  ],
  "bugs": "https://github.com/tnfe/hel/issues",
  "license": "MIT",
  "author": {
    "name": "fantasticsoul"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tnfe/hel.git",
    "directory": "packages/hel-micro"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "tsconfig.json"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "dist/index.iife.js",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "require": "./dist/index.js",
        "import": "./dist/index.mjs"
      },
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "build": "tsdown",
    "test": "pnpm --filter=hel-micro-core run build && jest"
  },
  "dependencies": {
    "hel-types": "workspace:*"
  },
  "devDependencies": {
    "@types/qs": "^6.9.7",
    "fake-indexeddb": "4.0.1",
    "qs": "^6.11.0",
    "tsdown": "latest"
  },
  "peerDependencies": {
    "hel-types": "workspace:*"
  },
  "bundleDependencies": [],
  "deprecated": false
}
